<div>
    <h1>Student List</h1>
    <table style="border: solid rgb(185, 79, 79) 1px;">
                <thead>
                    <tr style="border: solid rgb(185, 79, 79) 1px;;"> 
                        <th style= "border: solid rgb(185, 79, 79) 1px;">Student Id </th>
                        <th style="border: solid rgb(185, 79, 79) 1px;">Name </th>
                        <th style="border: solid rgb(185, 79, 79) 1px;">Program  </th>
                        <th style="border: solid rgb(185, 79, 79) 1px;">GPA </th>
                    </tr>
                </thead>
                <tbody>
                    {{#each students}}
                        <tr>
                            <td style="border: solid rgb(185, 79, 79) 1px;">{{this.studId}}</td>
                            <td style="border: solid rgb(185, 79, 79) 1px;">{{this.name}}</td>
                            <td style="border: solid rgb(185, 79, 79) 1px;">{{this.program}}</td>
                            <td style="border: solid rgb(185, 79, 79) 1px;">{{this.gpa}}</td>
                        </tr>
                    {{/each}}
                </tbody>
    </table>
    </div>